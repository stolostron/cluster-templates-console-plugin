    "@types/jest": "^29.2.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.0.0",
    "@types/react": "^17.0.37",
    "@types/react-helmet": "^6.1.4",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "i18next-parser": "^3.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "prettier": "^2.7.1",
    "prettier-stylelint": "^0.4.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.7.3",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "sass": "^1.58.0",
    "sass-loader": "^13.2.0",
    "showdown": "1.8.6",
    "style-loader": "^3.3.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "webpack": "^5.68.0",   
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "consolePlugin": {
    "name": "clustertemplates-plugin",
    "version": "0.0.1",
    "displayName": "Cluster Templates Plugin",
    "description": "Cluster Templates console plugin",
    "exposedModules": {
      "templates": "./components/ClusterTemplatesPage/ClusterTemplatesPage.tsx",
      "createTemplate": "./components/ClusterTemplatesPage/ClusterTemplateWizardPage.tsx",
      "editTemplate": "./components/ClusterTemplatesPage/ClusterTemplateEditPage.tsx",
      "templateDetails": "./components/ClusterTemplateDetailsPage/ClusterTemplateDetailsPage.tsx",
      "createQuota": "./components/Quotas/QuotaEditor/QuotaFormPage.tsx",
      "editQuota": "./components/Quotas/QuotaEditor/QuotaEditPage.tsx",
      "repositoriesContext": "./contexts/helmRepositoriesContext.ts"
    },
    "dependencies": {
      "@console/pluginAPI": "^4.11"
    }
  },
  "dependencies": {
    "@openshift-console/plugin-shared": "^0.0.3",
    "@patternfly/react-code-editor": "4.82.82",
    "@patternfly/react-icons": "^4.92.6",
    "@patternfly/react-table": "^4.111.4",
    "@types/humanize-plus": "^1.8.0",
    "classnames": "2.3.2",
    "formik": "^2.2.9",
    "formik-pf": "^0.0.1-alpha9",
    "humanize-plus": "^1.8.2",
    "i18next": "^21.8.14",
