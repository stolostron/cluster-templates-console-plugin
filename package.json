{
  "name": "clustertemplates-plugin",
  "version": "0.0.1",
  "private": true,
  "repository": "git@github.com/stolostron/cluster-templates-console-plugin.git",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && NODE_ENV=production yarn ts-node node_modules/.bin/webpack",
    "build-dev": "yarn clean && yarn ts-node node_modules/.bin/webpack",
    "start": "yarn ts-node node_modules/.bin/webpack serve",
    "start-console": "./start-console.sh",
    "i18n": "yarn i18next \"src/**/*.{js,jsx,ts,tsx}\" [-oc] -c i18next-parser.config.js",
    "ts-node": "ts-node -O '{\"module\":\"commonjs\"}'",
    "lint": "eslint src cypress && stylelint \"src/**/*.css\" --allow-empty-input",
    "lint:fix": "eslint src cypress -fix && stylelint \"src/**/*.css\" --allow-empty-input --fix",
    "podman:build": "podman build --file Dockerfile --tag caas-ui .",
    "podman:push": "podman tag caas-ui quay.io/$USER/caas-ui:latest && podman push quay.io/$USER/caas-ui:latest",
    "podman:deploy": "yarn podman:build && yarn podman:push quay.io/$USER/caas-ui:latest && ./scripts/deploy.sh quay.io/$USER/caas-ui:latest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "yarn run start-test 'yarn storybook --no-open' http://localhost:6006 'yarn cypress run'"
  },
